    "eslint": "eslint --ext .jsx --ext .js src/",
    "jest": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "autoprefixer": "10.2.4",
    "axios-mock-adapter": "1.19.0",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.2",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "intl": "1.2.5",
    "jest": "26.6.3",
    "postcss-loader": "4.2.0",
    "redux-mock-store": "1.5.4",
    "style-loader": "2.0.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "dayjs": "1.10.4",
    "history": "4.10.1",
    "lodash.clamp": "4.0.3",
    "lodash.merge": "4.6.2",
    "lodash.range": "3.2.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/src/mocks/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "./setup-tests.js"
